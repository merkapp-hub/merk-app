{
  "name": "MerkApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build-apk": "npx react-native run-android --mode=release",
    "signinreport": "cd android && ./gradlew signingReport",
    "pod": "cd ios && pod install && cd ..",
    "gradle-clean": "cd android && gradlew clean && cd ..",
    "pod-install": "cd ios && pod deintegrate && pod install && cd ..",
    "gradle-clean-mac": "cd android && ./gradlew clean && cd ..",
    "build-android": "cd android && gradlew clean && cd .. && react-native run-android",
    "release-buildAPK": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "release-buildAPK-window": "cd android && gradlew clean && gradlew assembleRelease && cd ..",
    "release-buildAAB": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "release-buildAAB-window": "cd android && gradlew clean && gradlew bundleRelease && cd ..",
    "remove-DerivedData": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && xcodebuild clean && cd ..",
    "gradle-permission": "cd android && chmod +x gradlew && cd ..",
    "debug-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew clean && ./gradlew assembleDebug && cd ..",
    "debug-build-window": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && gradlew clean && gradlew assembleDebug && cd ..",
    "android-log": "npx react-native log-android",
    "ios-log": "npx react-native log-ios"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-native": "0.82.0",
    "@react-native/new-app-screen": "0.82.0",
    "react-native-safe-area-context": "^5.5.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@sentry/react-native": "^7.1.0",
    "axios": "^1.11.0",
    "country-data": "^0.0.31",
    "country-state-city": "^3.2.1",
    "i18next": "^25.5.2",
    "nativewind": "^4.1.23",
    "react-i18next": "^15.7.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-feather": "^1.1.2",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-localize": "^3.5.2",
    "react-native-pager-view": "^6.9.1",
    "react-native-reanimated": "^4.0.3",
    "react-native-screens": "^4.15.4",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^15.13.0",
    "react-native-swiper-flatlist": "^3.2.5",
    "react-native-vector-icons": "^10.3.0",
    "react-native-wheel-color-picker": "^1.3.1",
    "react-native-worklets": "^0.6.0",
    "reanimated-color-picker": "^4.1.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}